{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "builds": [
    {
      "src": "**.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/**.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/send-email",
      "dest": "/api/send-email.js"
    }
  ]
}