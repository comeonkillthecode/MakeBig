{
  "version": 2,
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "/^(.*)\\.html$",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1.html",
      "status": 200,
      "use": "@vercel/static"
    },
    {
      "src": "/api/send-email.js",
      "use": "@vercel/node"
    }
  ]
}